{
    "rewrites": [
      { "source": "/(.*)", "destination": "/index.html" }
    ],
    "headers": [
      {
        "source": "/(.*)\\.js",
        "headers": [
          { "key": "Content-Type", "value": "application/javascript" }
        ]
      },
      {
        "source": "/(.*)\\.css",
        "headers": [
          { "key": "Content-Type", "value": "text/css" }
        ]
      },
      {
        "source": "/(.*)\\.(jpg|jpeg|png|gif|webp|svg|ico)",
        "headers": [
          { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
        ]
      }
    ]
}